		cacheModelsEnabled="true"
		enhancementEnabled="true"
		useStatementNamespaces="true"
		maxSessions="64"
		maxTransactions="8"
		maxRequests="128"/>
	
	<sqlMap resource="pe/gob/mincetur/report/ocex/model/dao/ibatis/maps/reporteInformeGestionMapper.xml"/>
	<sqlMap resource="pe/gob/mincetur/report/ocex/model/dao/ibatis/maps/reporteAccionPromocionMapper.xml"/>
	<sqlMap resource="pe/gob/mincetur/report/ocex/model/dao/ibatis/maps/reporteActividadMapper.xml"/>
	<sqlMap resource="pe/gob/mincetur/report/ocex/model/dao/ibatis/maps/reporteAlertaModificacionMapper.xml"/>
	<sqlMap resource="pe/gob/mincetur/report/ocex/model/dao/ibatis/maps/reporteAtencionConsultaMapper.xml"/>
	<sqlMap resource="pe/gob/mincetur/report/ocex/model/dao/ibatis/maps/reporteInformeActividadMapper.xml"/>
	
</sqlMapConfig>